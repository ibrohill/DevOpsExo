<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="statistique" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sn.dev.parrainageapp.controllers.StatistiqueController">
   <children>
      <BarChart layoutX="62.0" layoutY="84.0" prefHeight="232.0" prefWidth="498.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
      <Button fx:id="statistiquebtn" layoutX="14.0" layoutY="40.0" mnemonicParsing="false" onAction="#statistique" style="-fx-background-color: black;" text="Statistique" textFill="#f8f8f8">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="gestionCandidatbtn" layoutX="110.0" layoutY="40.0" mnemonicParsing="false" onAction="#statistique" style="-fx-background-color: black;" text="Gestion candidat" textAlignment="CENTER" textFill="#f8f8f8">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
